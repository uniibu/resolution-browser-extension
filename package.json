{
  "name": "ud-extension",
  "version": "3.1.1",
  "license": "MIT",
  "author": {
    "email": "eng@unstoppabledomains.com",
    "name": "Unstoppable Domains"
  },
  "homepage": "https://unstoppabledomains.com/extension",
  "scripts": {
    "clean": "rm -rf ./dist; rm -rf ./releases; rm -rf .parcel-cache",
    "postinstall": "node scripts/create-env.mjs && node ./scripts/create_manifest.js",
    "prettier": "prettier --write --log-level warn \"src/**/*.{js,jsx,json,ts,tsx,css,scss,md}\"",
    "predev": "yarn clean && yarn prettier",
    "dev": "NODE_ENV=staging; yarn clean && parcel watch src/manifest.json --host localhost --no-source-maps",
    "build": "yarn build:chrome",
    "build:dev": "NODE_ENV=staging; yarn build",
    "build:all": "yarn clean && yarn build:chrome:archives && yarn build:firefox:archives",
    "build:all:dev": "NODE_ENV=staging; yarn build:all",
    "build:extension": "parcel build src/manifest.json --no-cache --no-source-maps --no-scope-hoist",
    "build:chrome": "yarn build:chrome:manifest && yarn build:extension",
    "build:chrome:manifest": "node ./scripts/create_manifest.js chrome",
    "build:chrome:archives": "yarn build:chrome && yarn build:chrome:zip",
    "build:chrome:zip": "node scripts/create-zip.mjs chrome",
    "build:firefox": "yarn build:firefox:manifest && yarn build:extension",
    "build:firefox:manifest": "node ./scripts/create_manifest.js firefox",
    "build:firefox:archives": "yarn build:firefox && yarn build:firefox:zip",
    "build:firefox:zip": "node scripts/create-zip.mjs firefox"
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  },
  "dependencies": {
    "@metamask/providers": "^17.1.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@unstoppabledomains/config": "0.0.10",
    "@unstoppabledomains/ui-components": "0.0.42-experimental.59",
    "@unstoppabledomains/ui-kit": "0.3.24",
    "async-mutex": "^0.5.0",
    "eth-rpc-errors": "^4.0.3",
    "is-ipfs": "^8.0.4",
    "launchdarkly-react-client-sdk": "^3.3.2",
    "notistack": "^3.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.23.1",
    "ts-retry": "^5.0.1",
    "use-async-effect": "^2.2.7",
    "web3": "^4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@parcel/bundler-default": "^2.12.0",
    "@parcel/config-webextension": "^2.12.0",
    "@parcel/resolver-default": "^2.12.0",
    "@parcel/transformer-svg-react": "^2.12.0",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.14.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.5.0||^6.0.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "filenamify": "^6.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "https-browserify": "^1.0.0",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "path-browserify": "^1.0.0",
    "prettier": "^3.3.2",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^5.0.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.0",
    "typescript": "^5.4.5",
    "url": "^0.11.0",
    "vm-browserify": "^1.1.2",
    "zip-lib": "^1.0.4"
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  },
  "packageManager": "yarn@3.6.4",
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
